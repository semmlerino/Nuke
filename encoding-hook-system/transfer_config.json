{
  "include_patterns": [
    "*.py",
    "*.json",
    "*.yml",
    "*.yaml",
    "*.ini",
    "*.cfg",
    "*.pyi",
    "*.sh",
    "requirements*.txt",
    "Dockerfile",
    ".dockerignore",
    "pytest.ini",
    "pyrightconfig.json",
    "wrapper/*",
    "CLAUDE.md"
  ],
  "exclude_patterns": [
    "test_*.py",
    "*_test.py",
    "tests/",
    "Transfer.py",
    "transfer_cli.py",
    "bundle_app.py",
    "setup_transfer_hook.py",
    "*.log",
    "*.tmp",
    "*.bak",
    "*.backup",
    "*.backup.*",
    "*.old",
    "encoded_app_*.txt",
    "encoded_releases/*",
    "analyze_*.py",
    "count_*.py",
    "fix_*.py",
    "test_startup_fix.py",
    "shotbot_latest.txt",
    "venv_py311/*",
    "test_venv/*",
    "*AGENT*.md",
    "*WEEK*.md",
    "*REVIEW*.md",
    "*COMPLETE*.md",
    "*GUIDE*.md",
    "*PROGRESS*.md",
    "main_window_refactored.py",
    "*_simplified.py",
    "mock_strategy.py",
    "mock_workspace_pool.py",
    "recreate_vfx_structure.py",
    "recreate_mock_environment.bat",
    "join_and_recreate.py",
    "decode_app.py",
    "run_mock_*.py",
    "verify_*.py",
    "shotbot_mock.py",
    "threading_fix_verification.py",
    "run_tests*.py",
    "run_tests*.sh",
    "run_debug.sh",
    "run_fast_tests.sh",
    "run_full_tests.sh",
    "run_shotbot_mock.sh",
    "coverage*.json",
    "performance_optimizations.json",
    "startup_profile_results.json",
    "type_safety_performance_analysis.json",
    "PERFORMANCE_BASELINE.json",
    "test_dispatcher*.sh",
    "test_health_check.sh",
    "typecheck.sh",
    "demo_shots.json",
    "capture_vfx_structure.py",
    "screenshot*.py",
    "screenshot.sh",
    "take_screenshot.py",
    "auto_screenshot.py",
    "pyrightconfig.json.backup.json",
    "debug_utils.py",
    "transfer_config.json"
  ],
  "exclude_dirs": [
    "tests",
    "test",
    "__pycache__",
    ".git",
    ".pytest_cache",
    "venv",
    "venv_py311",
    "test_venv",
    "env",
    ".venv",
    ".env",
    "archive",
    "archived",
    "copy",
    ".shotbot",
    "htmlcov",
    "coverage_html",
    ".coverage",
    ".hypothesis",
    ".vscode",
    ".idea",
    "encoded_releases",
    "node_modules",
    "dist",
    "build",
    ".eggs",
    "docs",
    "dev-tools",
    "examples",
    ".post-commit-output",
    ".github",
    ".claude",
    ".serena"
  ],
  "max_file_size_mb": 10,
  "chunk_size_kb": 5120,
  "output_dir": "encoded_releases",
  "remote_branch": "encoded-releases",
  "auto_push": true,
  "clean_old_releases": true,
  "max_releases_to_keep": 10,
  "commit_message_template": "Auto-encoded release for commit {commit_hash}",
  "encoding_options": {
    "compression": "gzip",
    "include_metadata": true,
    "single_file_output": true
  }
}